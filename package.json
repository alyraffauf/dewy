{
	"name": "@alyraffauf/dewy",
	"version": "0.2.0",
	"license": "GPL-3.0-or-later",
	"bin": {
		"dewy": "dist/cli.js"
	},
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"test": "prettier --check . && xo && ava"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@doist/todoist-api-typescript": "^6.5.0",
		"@folder/xdg": "^4.0.1",
		"ink": "^6.7.0",
		"ink-link": "^5.0.0",
		"ink-spinner": "^5.0.0",
		"ink-text-input": "^6.0.0",
		"meow": "^14.0.0",
		"react": "^19.2.4"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^8.1.0",
		"@types/react": "^19.2.14",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^6.4.1",
		"chalk": "^5.6.2",
		"eslint-config-xo-react": "^0.29.0",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^7.0.1",
		"ink-testing-library": "^4.0.0",
		"prettier": "^3.8.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.9.3",
		"xo": "^1.2.3"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	},
	"prettier": "@vdemedes/prettier-config"
}
